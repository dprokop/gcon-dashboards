                  },
                  "custom": {
                    "align": "auto",
                    "cellOptions": {
                      "type": "color-background"
                    },
                    "inspect": false
                  },
                  "mappings": [
                    {
                      "options": {
                        "match": "true",
                        "result": {
                          "color": "dark-blue",
                          "index": 0
                        }
                      },
                      "type": "special"
                    },
                    {
                      "options": {
                        "match": "false",
                        "result": {
                          "color": "transparent",
                          "index": 1
                        }
                      },
                      "type": "special"
                    }
                  ],
                  "thresholds": {
                    "mode": "absolute",
                    "steps": [
                      {
                        "color": "green"
                      },
                      {
                        "color": "red",
                        "value": 80
                      }
                    ]
                  }
                },
                "overrides": []
              },
              "options": {
                "cellHeight": "sm",
                "footer": {
                  "countRows": false,
                  "fields": "",
