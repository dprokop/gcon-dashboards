              "queries": [
                {
                  "kind": "PanelQuery",
                  "spec": {
                    "datasource": {
                      "type": "grafana-testdata-datasource",
                      "uid": "PD8C576611E62080A"
                    },
                    "hidden": false,
                    "query": {
                      "kind": "grafana-testdata-datasource",
                      "spec": {
                        "csvContent": "start\napi\ngit\ndb\nok",
